<div class="container video-page">
  <div class="row">
    <div class="box small-12 medium-9 large-9 clearfix">
    <h2>VIDEOS</h2>
        <div class="videos clearfix">
      <% @videos.each do |v| %>
        <!-- <h4><%= v.title %></h4> -->
        <iframe width="560" height="315" src="//www.youtube.com/embed/<%= v.url %>" frameborder="0" allowfullscreen class="youtube"></iframe>
        <% if user_signed_in? %>
          <div>
            <%= button_to "Edit", edit_video_path(v.id), :method => "get", action: "edit", :class => "tiny radius button left" %>
            <%= button_to "Delete", video_path(v.id), :method => "delete", data: {confirm: "Are you sure?"}, :class => "tiny radius button left" %>
          </div>
        <% end %>
        <!-- <hr> -->
      <% end %>
      </div>
    </div>
  </div>
</div>

